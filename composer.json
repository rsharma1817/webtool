{
  "name": "fnbr/webtool",
  "description": "FrameNet Brasil Web Annotation Tool",
  "license": "GPLv3",
  "keywords": [
    "framenet",
    "annotation",
    "frames",
    "constructions"
  ],
  "authors": [
    {
      "name": "Tiago Torrent",
      "email": "tiago.torrent@gmail.com"
    },
    {
      "name": "Ely Matos",
      "email": "admin@ematos.net"
    }
  ],
  "require": {
    "psr/log": "1.0.0",
    "phpmailer/phpmailer": "~6.0",
    "aura/session": "^2.1",
    "beberlei/assert": "dev-master",
    "php-di/php-di": "5.2.*",
    "latte/latte": "2.4.*",
    "ext-json": "*",
    "ext-curl": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "*"
  },
  "autoload": {
    "files": [
      "core/classes/functions.php"
    ],
    "psr-4": {
      "database\\": "core/classes/database"
    },
    "classmap": [
      "core/classes/manager.php",
      "core/classes/interfaces",
      "core/classes/database",
      "core/classes/services",
      "core/classes/mvc",
      "core/classes/results",
      "core/classes/persistence",
      "core/classes/security",
      "core/classes/types",
      "core/classes/ui",
      "core/classes/utils"
    ]
  },
  "scripts": {
    "post-autoload-dump": [
      "Manager::postAutoloadDump"
    ]
  }
}
